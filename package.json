{
  "name": "ifix",
  "version": "1.0.0",
  "description": "A full-stack application with web dashboard and mobile app",
  "private": true,
  "workspaces": [
    "dashboard/frontend"
  ],
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "cd dashboard/frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd dashboard/frontend && npm run build",
    "start": "npm run start:frontend",
    "start:frontend": "cd dashboard/frontend && npm start",
    "install:frontend": "cd dashboard/frontend && npm install",
    "install:backend": "cd dashboard/backend && pip install -r requirements.txt",
    "setup": "npm run install:frontend && npm run install:backend",
    "backend": "cd dashboard/backend && python3 main.py",
    "dev:all": "concurrently \"npm run backend\" \"npm run dev:frontend\"",
    "lint": "cd dashboard/frontend && npm run lint",
    "type-check": "cd dashboard/frontend && npm run type-check"
  },
  "keywords": [
    "fullstack",
    "nextjs",
    "fastapi",
    "typescript",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}